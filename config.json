{
  "indentation": {
    "type": "spaces",
    "size": 4
  },
  "importFormat": {
    "spacesAroundImports": true
  },
  "maxLineLength": 80,
  "objectFormatting": {
    "maxPropertiesPerLine": 3,
    "trailingComma": true
  },
  "typeFormatting": {
    "maxPropertiesPerLine": 3,
    "trailingSemicolon": true
  },
  "arrayFormatting": {
    "maxElementsPerLine": 3,
    "trailingSemicolon": true
  }
}
